{
	"name": "obsidian-canvas-icons",
	"version": "1.0.0",
	"description": "Add icons from Lucide icon library directly to your Canvas",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"canvas",
		"icons",
		"lucide"
	],
	"author": "Canvas Icons Plugin",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/your-username/obsidian-canvas-icons.git"
	},
	"bugs": {
		"url": "https://github.com/your-username/obsidian-canvas-icons/issues"
	},
	"homepage": "https://github.com/your-username/obsidian-canvas-icons#readme",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}

